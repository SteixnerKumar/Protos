(variables
	(HOST_TYPE critical non_critical honeypot)
	(HOST_HAS_VULN yes no)
	(DECOY_PLACED yes no)
	(ATTACKER_PRIVS none user local_admin domain_admin)
)

(dd host_type_all
	(SAME HOST_TYPE)
)

(dd attacker_privs_local_priv_esc
	(ATTACKER_PRIVS'
		(none 	(0.0))
		(user 	(0.2))
		(local_admin 	(0.8))
		(domain_admin 	(0.0))
	)
)

(dd attacker_privs_domain_priv_esc_from_user
	(ATTACKER_PRIVS'
		(none	(0.0))
		(user 	(0.2))
		(local_admin 	(0.0))
		(domain_admin 	(0.8))
	)
)

(dd attacker_privs_domain_priv_esc_from_local_admin
	(ATTACKER_PRIVS'
		(none	(0.0))
		(user 	(0.0))
		(local_admin 	(0.2))
		(domain_admin 	(0.8))
	)
)

(dd attacker_privs_all
	(HOST_HAS_VULN
		(yes
			(ATTACKER_PRIVS
				(none
					(SAME ATTACKER_PRIVS)
				)
				
				(user (attacker_privs_local_priv_esc))
				(local_admin 	(SAME ATTACKER_PRIVS))
				(domain_admin 	(SAME ATTACKER_PRIVS))
			)
		)
		
		(no
			(SAME ATTACKER_PRIVS)
		)
	)
)

(env

	(actiondbn scan_host
			
	)
	
	(actiondbn priv_esc
		(ATTACKER_PRIVS 	(attacker_privs_all))
	)
)